<div class="container">
  <div class="jumbotron">
    <h3> New Question: </h3>
    {{new-question saveQuestion="questionToDb"}}
  </div>
  <div class="subsection">
    <ul>
      {{#each model.questions as |question|}}
         {{#link-to 'question' question.id}}<li class="subsectionBullet">{{question.content}} - {{question.author}}</li>{{/link-to}}
      {{/each}}
    </ul>
  </div>
</div>
